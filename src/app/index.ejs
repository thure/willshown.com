<!doctype html>
<html>
<head>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <title>Will Shown &#x2661;</title>

    <% if(typeof watch !== 'undefined' && watch){ %>
    <script src="//localhost:<%= watch %>/livereload.js"></script>
    <% } %>

    <% if(typeof prod !== 'undefined' && prod) { %>
    <script src="main.min.js"></script>
    <link rel="stylesheet" href="main.min.css"/>
    <% }else{ %>
    <script src="js/lib/require.js" data-main="js/main.js"></script>
    <link rel="stylesheet" href="style/main.css"/>
    <% } %>

</head>
<body data-displaying="front">

<main>

    <section data-page="front">
        <div class="splash">
            <h1 class="row">
                <%- assets['Will Shown'] %>
            </h1>
            <div class="row horizontal">
                <i class="loading" data-loaded="0">
                    <%- assets['loading'] %>
                    <div class="overlay">
                        <button class="go" disabled>
                            <p>Open Portfolio</p>
                        </button>
                    </div>
                </i>
            </div>
        </div>
        <div class="portfolio">
            
        </div>
    </section>

    <nav class="main">
        <button class="toggle">
            <p class="front">about / résumé / contact <i class="chevron down"></i></p>
            <p class="back">portfolio <i class="chevron up"></i></p>
        </button>
    </nav>

    <section data-page="back">

    </section>

</main>

</body>
</html>